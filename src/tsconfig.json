{
    "compilerOptions": {
        "strictNullChecks": true,
        "noImplicitAny": true,
        "module": "CommonJS",
        "target": "ES2020",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strict": true,
        "strictPropertyInitialization": true,
        "lib": ["ES2020"],
        // "baseUrl": "./",
        // "paths": {
        //     // 模块名到基于 baseUrl 的路径映射的列表
        //     "@/*": ["src/*"]
        // },
        "types": ["node"],
        "typeRoots": ["typings"]
    },
    "include": ["./src/*", "./**/*.ts", "health-package/pages/fast-choose-time/index.js", "component/time-picker/index.js", "user-package/pages/nickname/nickname.js", "user-package/pages/invite_code/invite_code.js"],
    "exclude": ["node_modules", "dist"]
}
